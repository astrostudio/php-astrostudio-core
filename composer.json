{
    "name": "astrostudio/php-astrostudio-core",
    "description": "Utility function",
    "type": "library",
    "license": "proprietary",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "composer/installers": "*"
    },
    "autoload": {
        "psr-4": {
            "AstroStudio\\Core\\": "src"
        }
    },
    "extra": {
        "installer-name": "php-astrostudio-core"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.5",
        "squizlabs/php_codesniffer": "^3.13"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
